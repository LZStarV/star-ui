{
  "name": "@star-ui/core",
  "version": "0.0.1",
  "type": "module",
  "description": "Core functionality for Star UI",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./style": {
      "import": "./dist/style.css"
    },
    "./src/styles/index.scss": {
      "import": "./src/styles/index.scss"
    }
  },
  "scripts": {
    "build": "tsc && vite build",
    "dev": "vite build --watch"
  },
  "dependencies": {
    "@star-ui/shared": "workspace:*"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  }
}
